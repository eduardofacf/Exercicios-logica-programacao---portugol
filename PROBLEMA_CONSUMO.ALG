Algoritmo "problema_consumo"

Var

   distancia, combGasto, consumoMedio : real

Inicio

   escreva("Distancia pecorrida: ")
   leia(distancia)
   escreva("Combustivel gasto: ")
   leia(combGasto)
   
   consumoMedio <- distancia / combGasto

   escreval("Consumo medio: ", consumoMedio:5:3)


Fimalgoritmo